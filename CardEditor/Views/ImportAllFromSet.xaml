<Window x:Class="CardEditor.Views.ImportAllFromSet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CardEditor.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="30*"/>
        </Grid.RowDefinitions>
        <Label Content="Pokemontcg.io URL"/>
        <TextBox x:Name="urlTextbox" Grid.Row="1"/>
        <DockPanel Grid.Row="2">
            <Button Width="90" Margin="5,0,0,0" Content="Import" Click="Button_Click"/>
            <Button Width="90" Content="Cancel" Click="Button_Cancel_Click"/>
        </DockPanel>
    </Grid>
</Window>
